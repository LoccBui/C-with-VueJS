<script type="module">
    import { createApp } from 'https://unpkg.com/vue@3/dist/vue.esm-browser.js'

    createApp({

    data() {
    return {
    message: 'Hello Vue!',
    info: []
    }
    },
    methods: {
    nhan(){
        axios.get('http://localhost:64463/user/get')
            .then(response => this.info.push(response.data))
    }
    }
    }).mount('#app')
</script>

