@{
    ViewBag.Title = "Home Page";
}

<div id="app">

    <div class="login-cover">
            <h1>Đăng nhập</h1>
            <input placeholder="Nhập tài khoản" />
            <input placeholder="Nhập mật khẩu"/>
    </div>


    <button v-on:click="nhan"> Đăng nhập </button>
    <div v-for="item of info">
        <p>{{item.username}}</p>
        <p>{{item.password}}</p>
    </div>

</div>

